{
  "expo": {
    "name": "Juggle Sports",
    "slug": "Juggle-Sports",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/soccer.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./images/soccer.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.konjo.jugglesports",
      "buildNumber": "2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./images/soccer.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.konjo.jugglesports"
    },
    "extra": {
      "eas": {
     
      }
    }
  }
}
